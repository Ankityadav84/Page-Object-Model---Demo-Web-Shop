<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">

	<listeners>
		<listener class-name="utils.SuiteListener"></listener>
	</listeners>

	<test thread-count="10" name="Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>

			<class name="qa.tests.VerifyTopMenu"></class>
			<class name="qa.tests.VerifyCategories"></class>
			<class name="qa.tests.VerifyLoginPage"></class>
			<class name="qa.tests.VerifyOtherSections"></class>
			<class name="qa.tests.VerifyRegisterPage"></class>
		</classes>
	</test> <!-- Test -->

</suite> <!-- Suite -->
